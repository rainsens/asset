{
    "name": "rainsens/asset",
    "description": "A php website asset package based upon Laravel",
    "authors": [
        {
            "name": "Rainsen",
            "email": "yusen@rainsen.com"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "ext-json": "*",
        "laravel/framework": "^6.20"
    },
    "autoload": {
        "psr-4": {
            "Rainsens\\Asset\\": "src"
        },
        "files": [
            "bootstrap/helpers.php"
        ]
    },
    "require-dev": {
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rainsens\\Asset\\Providers\\AssetServiceProvider"
            ]
        },
        "aliases": {
            "Asset": "Rainsens\\Asset\\Facades\\Asset"
        }
    }
}
